{
	"root": true,
	"env": {
		"browser": true,
		"node": true,
		"es2021": true
	},
	"extends": [
		"eslint:recommended",
		"plugin:vue/vue3-recommended",
		"@vue/typescript/recommended",
		"prettier"
	],
	"rules": {
		"no-undef": "off",
		"vue/multi-word-component-names": "off",
		"vue/no-v-html": "off",
		"@typescript-eslint/ban-ts-comment": "off"
	}
}
